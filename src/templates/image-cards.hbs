{{#each this}}
<div class="photo-card">
    <img class="gallery-image" src="{{webformatURL}}" alt="{{tages}}" />
    
    <div class="stats">
        <p class="stats-item">
            <i class="material-icons gray08">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</div>
{{/each}}


